# Bluetooth
genfscon proc /bt_mac                                         u:object_r:proc_bt_mac:s0

# LED
genfscon sysfs /devices/soc/800f000.qcom,spmi/spmi-0/spmi0-03/800f000.qcom,spmi:qcom,pmi8998@3:qcom,leds@d000/leds/rgb/rgb_blink u:object_r:sysfs_graphics:s0

# Radio
genfscon proc /bandinfo                                       u:object_r:proc_bandinfo:s0

# Sensors
genfscon proc /baseband                                       u:object_r:proc_baseband:s0
